{
  "name": "rental-scraper-app",
  "version": "1.0.0",
  "description": "Web app to scrape rental listings from VRBO/Booking.com/Airbnb and send to Notion via Zapier",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "dev:headed": "cross-env HEADED=true node --watch server.js",
    "start:headed": "cross-env HEADED=true node server.js",
    "install-browsers": "npx playwright install chromium",
    "postinstall": "npx playwright install chromium"
  },
  "keywords": [
    "rental",
    "scraper",
    "playwright",
    "zapier",
    "notion"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "formidable": "^3.5.1",
    "playwright": "^1.48.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
